{
  "version": 2,
  "builds": [
    {
      "src": "Frontend-Body",
      "use": "@vercel/static-build",
      "config": { "distDir": "Frontend-Body/dist" }
    },
    {
      "src": "Backend-Soul",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Frontend-Body/dist/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/Backend-Soul/dist/index.js"
    }
  ]
}
