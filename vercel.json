{
  "builds": [
    {
      "src": "/Backend-Soul/**",
      "use": "@vercel/node"
    },
    {
      "src": "/Frontend-Body/**",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend-Soul/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend-Body/$1"
    }
  ]
}
